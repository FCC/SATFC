                strategy: 
                  name: ITERATIVE_DEEPEN
                  args:
                    scalingFactor: 0
                    baseCutoff: 10
                    config: 
                      name: NEIGHBOURHOOD
                      args: 
                        numLayers: 5000
          -
            - name: CLASP
              args: 
                config: --backprop --eq=0 --trans-ext=all --sat-prepro=0 --sign-def=0 --del-max=100000 --strengthen=local,1 --loops=common --init-watches=0 --heuristic=Vsids,96 --del-cfl=F,500 --restarts=D,100,0.8,100 --update-act --del-glue=4,0 --update-lbd=0 --reverse-arcs=3 --otfs=2 --del-on-restart=0 --contraction=500 --local-restarts --lookahead=no --save-progress=50
                encodingType: DIRECT
          -
            - name: CLASP
              args: 
                config: --backprop --eq=0 --trans-ext=all --sat-prepro=0 --sign-def=0 --del-max=100000 --strengthen=local,1 --loops=common --init-watches=0 --heuristic=Vsids,96 --del-cfl=F,500 --restarts=D,100,0.8,100 --update-act --del-glue=4,0 --update-lbd=0 --reverse-arcs=3 --otfs=2 --del-on-restart=0 --contraction=500 --local-restarts --lookahead=no --save-progress=50
                encodingType: DIRECT
            - name: SAT_CACHE
            - name: VERIFIER
            - name: CACHE
            - name: CONNECTED_COMPONENTS
            - name: UNDERCONSTRAINED
            - name: ARC_CONSISTENCY
          -
            - name: UNSAT_PRESOLVER
              args:     
                solverConfig:
                  name: CLASP
                  args:
                    config: --backprop --eq=0 --trans-ext=all --sat-prepro=0 --sign-def=0 --del-max=100000 --strengthen=local,1 --loops=common --init-watches=0 --heuristic=Vsids,96 --del-cfl=F,500 --restarts=D,100,0.8,100 --update-act --del-glue=4,0 --update-lbd=0 --reverse-arcs=3 --otfs=2 --del-on-restart=0 --contraction=500 --local-restarts --lookahead=no --save-progress=50
                    encodingType: DIRECT
                strategy: 
                  name: ITERATIVE_DEEPEN
                  args:
                    scalingFactor: 0
                    baseCutoff: 10
                    config: 
                      name: NEIGHBOURHOOD
                      args: 
                        numLayers: 5000
    - name: RESULT_SAVER
    - name: VERIFIER
    - name: CACHE
VHF:
    - name: CLASP
      args: 
        config: --backprop --eq=0 --trans-ext=all --sat-prepro=0 --sign-def=0 --del-max=100000 --strengthen=local,1 --loops=common --init-watches=0 --heuristic=Vsids,96 --del-cfl=F,500 --restarts=D,100,0.8,100 --update-act --del-glue=4,0 --update-lbd=0 --reverse-arcs=3 --otfs=2 --del-on-restart=0 --contraction=500 --local-restarts --lookahead=no --save-progress=50
        encodingType: DIRECT
    - name: CONNECTED_COMPONENTS
    - name: UNDERCONSTRAINED
      args: 
        expensive: false
        recursive: false
    - name: SAT_PRESOLVER
      args: 
        solverConfig:
          name: CLASP
          args:
            config: --backprop --eq=0 --trans-ext=all --sat-prepro=0 --sign-def=0 --del-max=100000 --strengthen=local,1 --loops=common --init-watches=0 --heuristic=Vsids,96 --del-cfl=F,500 --restarts=D,100,0.8,100 --update-act --del-glue=4,0 --update-lbd=0 --reverse-arcs=3 --otfs=2 --del-on-restart=0 --contraction=500 --local-restarts --lookahead=no --save-progress=50
            encodingType: DIRECT
        strategy: 
          name: ITERATIVE_DEEPEN
          args:
            scalingFactor: 0
            baseCutoff: 10
            config: 
              name: NEIGHBOURHOOD
              args: 
                numLayers: 1
    - name: RESULT_SAVER
    - name: VERIFIER