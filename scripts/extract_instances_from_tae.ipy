index = '~/arrow/afrechet/experiments/satfc-paper/instances/april27-instances/2909/UHF_CNF_Index.json'
suffix = '-2909'
base_path = '~/arrow/afrechet/experiments/satfc-paper/configuration/output/camera/2909/'
for tae in ['TAEEvaluationOutput-clasp-h2-description.csv', 'TAEEvaluationOutput-DCCA+-description.csv', 'TAEEvaluationOutput-clasp-h1-description.csv']:
    algname = tae.replace('TAEEvaluationOutput-','').replace('-description.csv','')
    tae_file_path = base_path + tae
    output = base_path + algname + suffix + '.csv'
    !python replaceTAEInstanceName.py -cnf_to_srpk_index $index -input $tae_file_path -output $output
    if 'preSAT' in algname: # No components to deal with
        continue
    component_file = output + '-components.csv'
    !cat $output | grep 'component\|Seed' > $component_file
    !cat $output | grep -v component > tmp
    !rm $output
    !cp tmp $output
    !rm tmp